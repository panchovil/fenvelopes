&nml_setup
    nc=3,                ! Number of components
    model="SRK",        ! SRK PR76 PR78 RKPR
    mixrule="ClassicVdW" ! ClassicVdW 
/

&nml_composition
    names="CH4" "C50-C64" "Asphaltenes"
    spec="critical", ! critical or parameters
